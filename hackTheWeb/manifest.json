{
    "manifest_version": 3,
    "name": "HackTheWeb",
    "description": "After adding the extension, you can edit the content",
    "version": "1.0",
    "icons": {
        "128": "hack.png"
    },
    "action": {
        "default_icon": {
            "128": "hack.png"
        }
    },
    "content_scripts": [{
        "js": [
            "main.js"
        ],
        "matches": [
            "https://www.google.com/*"
        ]
    }]
}
